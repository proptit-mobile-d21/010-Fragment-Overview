<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="0" android:toDegrees="0">
            <shape
                android:innerRadiusRatio="2.75"
                android:shape="ring"
                android:thickness="28dp"
                android:useLevel="true">
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>
    <item android:left="210dp">
        <shape
            android:innerRadiusRatio="1000"
            android:shape="ring"
            android:thickness="14dp"
            android:useLevel="false">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item>
        <rotate>
            <inset android:insetLeft="210dp">
                <shape
                    android:innerRadiusRatio="1000"
                    android:shape="ring"
                    android:thickness="14dp"
                    android:useLevel="false">
                    <solid android:color="@color/white" />
                </shape>
            </inset>
        </rotate>
    </item>
</layer-list>